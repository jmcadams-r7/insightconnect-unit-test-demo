#!/usr/bin/env python
# GENERATED BY KOMAND SDK - DO NOT EDIT
import komand
from icon_unit_test_demo import connection, actions, triggers


Name = "Unit Test Demo"
Vendor = "rapid7"
Version = "1.0.0"
Description = "Show people how to unit test stuff"


class ICONUnitTestDemo(komand.Plugin):
    def __init__(self):
        super(self.__class__, self).__init__(
                name=Name, 
                vendor=Vendor, 
                version=Version,
                description=Description,
                connection=connection.Connection()
                )
        self.add_trigger(triggers.RandomNumber())

        self.add_action(actions.AddNumbers())


def main():
    """Run plugin"""
    cli = komand.CLI(ICONUnitTestDemo())
    cli.run()


if __name__ == "__main__":
    main()
